{
  "name" : "elastic/elastic-client-codegen-php",
  "description" : "A base library used to create PHP clients at Elastic.",
  "homepage": "https://github.com/elastic/elastic-client-codegen-php",
  "keywords" : [
    "php",
    "client"
  ],
  "license" : "Apache-2.0",
  "type" : "library",
  "authors" : [
    {"name" : "Aur√©lien FOUCRET", "email": "aurelien.foucret@elastic.co"}
  ],
  "require" : {
    "php" : "^5.6|^7.0",
    "guzzlehttp/ringphp" : "^1.0",
    "psr/log" : "^1.0."
  },
  "require-dev" : {
    "phpunit/phpunit" : "6.3.0",
    "squizlabs/php_codesniffer" : "^3.4.0",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "autoload" : {
    "psr-4" : {
      "Elastic\\Client\\" : ""
    },
    "exclude-from-classmap" : [
      "/Tests/"
    ]
  },
  "bin": [
      "resources/dev/elastic-codegen.sh"
  ]
}
